{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "indent": [true, "spaces", 4],
        "quotemark": [true, "double", "jsx-double"],
        "max-classes-per-file": false,
        "max-line-length": false,
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "member-access": false,
        "trailing-comma": false,
        "arrow-parens": false,
        "no-console": false
    },
    "rulesDirectory": []
}